{
  "manifest_version": 3,
  "name": "fbp",
  "version": "0.0",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "storage",
    "http://localhost:18080/",
    "tabs",
    "activeTab"
  ], 
  "action": {
    "default_popup": "index.html",
    "default_icon": "images/icon-32.png"
  }
}
